            <field name="state">code</field>
            <field name="active">True</field>
            <field name="code">model._onchange_physician(record.id)</field>
        </record>

        <record id="sales_order_update_amount" model="base.automation">
            <field name="name">Sales Order: Update Amount</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="trigger">on_create_or_write</field>
            <field name="state">code</field>
            <field name="active">True</field>
            <field name="code">if record.clinic_booking_id and record.state not in ('draft','done','cancel'):
            model.env['clinic.booking']._update_amount_due(record.clinic_booking_id.id, record.amount_total)</field>
        </record>

